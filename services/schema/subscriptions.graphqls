interface Event {
	id: ID!
}

type BlockEvent implements Event {
	id: ID!
	block: Int!
	logs: Int!
	simulated: Boolean!
}

type Subscription {
	events(gameID: ID!, simulated: Boolean): Event!
	transaction(gameID: ID!, owner: String): ActionTransaction!
	session(gameID: ID!, owner: String): Session!
}
